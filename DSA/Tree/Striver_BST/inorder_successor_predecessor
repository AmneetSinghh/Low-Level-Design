node inorderSuccessor(node root , int target){
    node successor = null
    while(root!=null){
        if(root->val>=target){
            successor = root->val;
            root = root->left;
        }
        else{
            root = root->right;
        }
    }
    return successor;
}


node inorderPredecessor(node root , int target){
    node predecessor = null
    while(root!=null){
        if(root->val>=target){
            root = root->left;
        }
        else{
            predecessor = root->val;
            root = root->right;
        }
    }
    return successor;
}





