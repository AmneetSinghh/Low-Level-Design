public node lowestCommonAncestor(node root, node first, node second){
    if(root== null) return null;
    int cur = root->val;
    if(cur< first.val and cur< second.val){
        return lowestCommonAncestor(root->right,first,second);
    }
    if(cur> first.val and cur> second.val){
        return lowestCommonAncestor(root->left,first,second);
    }
    return;
}

vector<int> vc;

// time complexity O(H)